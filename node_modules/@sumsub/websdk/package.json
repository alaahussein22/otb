{
  "name": "@sumsub/websdk",
  "version": "1.4.1",
  "description": "SumSub WebSdk",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "types"
  ],
  "author": "SumSub",
  "license": "MIT",
  "keywords": [
    "sumsub",
    "web",
    "websdk"
  ],
  "devDependencies": {
    "@testing-library/dom": "8.1.0",
    "@types/jest": "27.0.1",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-todo-plz": "1.3.0",
    "jest": "27.0.6",
    "jest-environment-jsdom": "27.0.6",
    "jest-junit": "12.2.0",
    "ts-jest": "27.0.5",
    "tslib": "2.3.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint",
      "prettier --ignore-path ../../.prettierignore --config ../../.prettierrc.json --write"
    ]
  },
  "scripts": {
    "build": "pnpm run build:bundle && pnpm run build:types && pnpm run copyCdn",
    "build:bundle": "rollup -c",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "copyCdn": "cp dist/index.js ../../iframe/public/static/sns-websdk-builder.js",
    "test": "pnpm run test:unit",
    "test:unit": "jest",
    "lint": "eslint ./src/* --report-unused-disable-directives",
    "precommit": "lint-staged -q",
    "prepush": "pnpm run test:unit",
    "format": "prettier --ignore-path ../../.prettierignore --config ../../.prettierrc.json --write ."
  },
  "typings": "types"
}